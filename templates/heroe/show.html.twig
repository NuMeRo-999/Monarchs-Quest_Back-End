{% extends 'base.html.twig' %}

{% block title %}Heroe{% endblock %}

{% block body %}
    <h1>Heroe</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ heroe.id }}</td>
            </tr>
            <tr>
                <th>HealthPoints</th>
                <td>{{ heroe.healthPoints }}</td>
            </tr>
            <tr>
                <th>AttackPower</th>
                <td>{{ heroe.attackPower }}</td>
            </tr>
            <tr>
                <th>CriticalStrikeChance</th>
                <td>{{ heroe.criticalStrikeChance }}</td>
            </tr>
            <tr>
                <th>Defense</th>
                <td>{{ heroe.defense }}</td>
            </tr>
            <tr>
                <th>Experience</th>
                <td>{{ heroe.experience }}</td>
            </tr>
            <tr>
                <th>Level</th>
                <td>{{ heroe.level }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ heroe.state }}</td>
            </tr>
            <tr>
                <th>MaxHealthPoints</th>
                <td>{{ heroe.maxHealthPoints }}</td>
            </tr>
            <tr>
                <th>ImageFilename</th>
                <td>{{ heroe.imageFilename }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ heroe.name }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_heroe_index') }}">back to list</a>

    <a href="{{ path('app_heroe_edit', {'id': heroe.id}) }}">edit</a>

    {{ include('heroe/_delete_form.html.twig') }}
{% endblock %}
